/**index.wxss**/
page{
  background: #f1f2f4;
  font-family: "微软雅黑";
}
/* 搜索框 */
.search-nav{
  width:100%;
  margin:0rpx auto;
  background: #fff;
  display: flex;
  padding-top:5rpx;
  padding-bottom:20rpx;
}
.more-city{
  width:140rpx;
  margin-left:25rpx;
  height: 60rpx;
  align-items: center;
  font-size:28rpx;
  /* line-height: 60rpx; */
  }
.more-city .picker{
  width:100%;
  display: flex;
  align-items: center;
}
.more-city image{
  width:17rpx;
  height: 10rpx;
  display: inline-block;
  margin-left:20rpx;
}
.search-container{
  width:550rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 120rpx;
  background: #f4f4f4;
  display: flex;
}
.search-container image{
  width: 25rpx;
  height: 24rpx;
  margin-top:15rpx;
  margin-left:45rpx;
}
.search-container input{
  width:560rpx;
  font-size:24rpx;
  color:#a6a6a6;
  margin-left:20rpx;
  margin-top:4rpx;
}
/* 轮播图 */
.swiper {
  height: 280rpx;
  width: 100%;
}
.swiper image {
  height: 100%;
  width: 100%;
}
/* 导航 */
.nav-box{
  width:100%;
  margin:10rpx auto;
  background: #fff;
  padding-top:5rpx;
  padding-bottom:10rpx;
}
.nav-container{
  width:675rpx;
  display: flex;
  margin:0 auto;
  justify-content: space-between;
  margin-top:20rpx;
}
.nav-container .nav-item{
  width:100rpx;
  font-size:24rpx;
  color:#a5a5a5;
} 
.nav-container .nav-item button{
  width:100rpx;
  padding:0rpx;
  margin:0rpx;
  background: #fff;
  font-size:24rpx;
  color:#333;
}
.nav-container .nav-item button::after{
  border: none;
}
.nav-container .nav-item image{
  width:100rpx;
  height: 100rpx;
  margin-bottom:-25rpx;
}
/* 账目 */
.account{
  width:100%;
  background: #fff;
  display: flex;
  margin:10rpx auto;
}
.account .title{margin:5rpx 10rpx;font-size:32rpx;color:#333333;font-weight: bold;}
.account .subtitle{font-size:24rpx;color:#888888;margin:5rpx 10rpx;}
.account .left{
  width:370rpx;
  height: 400rpx;
  border-right: 2rpx solid #ddd;
  /* background: url(http://127.0.0.1/img/user/index-01.png) top center; */
  /* background-size:100% 100%;  */
}
.account .left image{
  width:370rpx;
  height: 400rpx;
}
.account .right{
  width:370rpx;
  height: 400rpx;
}
.account .right .top{
  width:100%;
  height: 200rpx;
  border-bottom: 2rpx solid #ddd;
   /* background: url(http://127.0.0.1/img/user/index-02.png) top center; */
  /* background-size:100% 100%;  */
}
.account .right .top image{
  width:100%;
  height: 200rpx;
}
.account .right .bottom{
  width:100%;
  height: 200rpx;
   /* background: url(http://127.0.0.1/img/user/index-03.png) top center; */
  /* background-size:100% 100%;  */
}
.account .right .bottom image{
  width:100%;
  height: 200rpx;
}
/* 精选专题 */
.special{
  width:100%;
  background: #fff;
}
.special .title{
  width:700rpx;
  margin:0rpx auto;
  align-items: center;
  display: flex;
  padding-top:20rpx;
  padding-bottom:20rpx;
  justify-content: space-between;
}
.special .title text{
  font-size:32rpx;font-weight: bold;color:#333;
}
.special .title image{
  width:16rpx;
  height: 30rpx;
}
.special .image-container{
  width:750rpx;
  display: flex;
  height: 280rpx;
  overflow: auto;
}
.special .image-container .img-item{
  display: block;
  width:320rpx;
  height: 200rpx;
  position: relative;
  border-radius: 15rpx;
  margin-left:20rpx;
  margin-top:30rpx;
}
.special .image-container .img-item text{
  position: absolute;
  width:260rpx;
  height: 140rpx;
  text-align: center;
  font-size:28rpx;
  line-height: 30rpx;
  padding:30rpx;
  border-radius: 15rpx;
  top:0rpx;
  left:0rpx;
  /* display: block; */
  background: rgba(0, 0, 0 , 0.3);
  color:#fff;
  display: table-cell;
  vertical-align:middle;
}
.special .image-container .img-item image{
  width:320rpx;
  height: 200rpx;
}
/* 推荐企业 */
.resume{
  width:100%;
  background: #fff;
  margin-top:10rpx;
}
.resume .title{
  width:700rpx;
  margin:0rpx auto;
  align-items: center;
  display: flex;
  padding-top:20rpx;
  padding-bottom:20rpx;
  justify-content: space-between;
}
.resume .title text{
  font-size:32rpx;font-weight: bold;color:#333;
}
.resume .title image{
  width:16rpx;
  height: 30rpx;
}
.resume .list-container{
  width:100%;
}
.resume .list-container .list-item{
  width:700rpx;
  display: flex;
  margin:20rpx auto;
}
.resume .list-container .list-item .left{
  width:150rpx;
}
.resume .list-container .list-item .left image{
  width:120rpx;
  height: 120rpx;
}
.resume .list-container .list-item .right{
  width:550rpx;
}
.resume .list-container .list-item .right .title{
  width:550rpx;
  height: 30rpx;
  font-size:28rpx;
  color:#333;
  font-weight: bold;
  padding:0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.resume .list-container .list-item .right .subtitle{
  font-size:24rpx;
  margin-top:20rpx;
  color:#888;
  padding:0;
}
.resume .list-container .list-item .right .sign-container{
  width:100%;
  display: flex;
  border-radius: 5rpx;
  margin-top:20rpx;
  overflow: hidden;
}
.resume .list-container .list-item .right .sign-container text{
  display: block;
  font-size:22rpx;
  padding:2rpx 10rpx;
  margin-right: 10rpx;
}
.resume .list-container .list-item .right .sign-container .y{
  color:#ff9833;
  background: #fff4ea;
}
.resume .list-container .list-item .right .sign-container .r{
  color:#ee3112;
  background: #fdeae7;
}
.resume .list-container .list-item .right .sign-container .b{
  color:#0090ec;
  background: #eaf1ff;
}
/* 底部布局 */
.footer{
  width:100%;
  position:fixed;
  bottom:0;
  height: 100rpx;
 background: #fff;
 border-top:2rpx solid #dddddd;
  display: flex;
  justify-content: space-between;
}
.footer .item{
  width:33.33%;
  text-align: center;
}
.footer .item button{
  width:33.33%;
  text-align: center;
  background: #fff;
  padding:0rpx;
  
}
.footer .item button::after{
  border: none;
}
.footer .item image{
  /* margin-top:10rpx; */
  width:42rpx;
  height: 42rpx;
  /* margin-bottom:-20rpx; */
}
.footer .item text{
  margin-top:-43rpx;
  display: block;
  font-size:22rpx;
  color:#555555;
}
.footer button .active{color:#0090ec;}

.footer .active button::after{
  border:none;
}
#fixed-top{
  position: fixed;
  top:-22rpx;
  width:100%;
  height:112rpx;
  background:#fff;
  z-index:1000000;
}
#fixed-top .search-container{
  margin:24rpx auto;
}
/* 省市级联 */
.citypickers{
  position: fixed;
  height: 100%;
  width: 100%;
  min-height: 100%;
  background-color: red;
}


.citybody {
  position: fixed;
  bottom: 0px;
  background: #fff;
}

.cityheader {
  position: absolute;
  top:0px;
  width: 100%;
  z-index: 4;
}

.city-cancel {
  float: left;
  margin: 20rpx;
  color: #818181;
}

.city-true {
  float: right;
  margin: 20rpx;
  color: #2FB42E
}

.section .picker {
  background-color: #fff;
  border-bottom: 1px #d9d9d9 solid;
  border-top: 1px #d9d9d9 solid;
  padding: 20rpx;
}
/*  用户认证  */
.auth-view{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .6);
  z-index: 9999;
}
.authorize{
  background: none;
  box-shadow: none;
  margin: 140rpx auto 0;
  width: 600rpx;
  height: 800rpx;
  padding: 0;
}
.authorize image{
  width: 100%;
  height: 100%;
}

.authorize-btn{
  height: 75rpx;
  line-height: 75rpx;
  font-size: 25rpx;
  background: #f98d4f;
  color: #FFF;
}
.border-top{
  border-top:2rpx solid #f2f2f2;
}